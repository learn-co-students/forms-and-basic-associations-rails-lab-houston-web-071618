<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <p><%= f.label :title %>
  <%= f.text_field :title %></p>
  <p><%= f.label :artist_name %>
  <%= f.text_field :artist_name %></p>
  <p><%= f.label :genre_name %>
  <%= f.collection_select :genre_name, Genre.all, :id, :name %></p>
  <p>Note 1:<input name='song[note_contents][]' id='song_notes_1'></p>
  <p>Note 2:<input name='song[note_contents][]' id='song_notes_2'></p>
  <p>Note 3:<input name='song[note_contents][]' id='song_notes_3'></p>


  <%= f.submit %>
<% end %>
